      "source": [
        "#write a program to display all prime numbers between 1 to 100\n",
        "def is_prime(num):\n",
        "  if num<2:\n",
        "    return False\n",
        "  for i in range(2 , int(num** 0.5)+1):\n",
        "    if num%i==0:\n",
        "      return False\n",
        "  return True\n",
        "for num in range(1, 101):\n",
        "  if is_prime(num):\n",
        "    print(num, end =\" \")"
      ]
